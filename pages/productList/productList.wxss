/* pages/productList/productList.wxss */


.navbar {
  flex: none;
  display: flex;
  background: #fff;
}

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
  font-size: 14px;
}

/* 顶部导航字体颜色 */

.navbar .item.active {
  color: #69C3AA;
}

/* 顶部指示条属性 */

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 6rpx;
  background: #69C3AA;
}
.weui-media-box__title{
  font-size: 50rpx;
}
.placeholder1{
  color: red;
}