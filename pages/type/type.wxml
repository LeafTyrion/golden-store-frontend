<view class="weui-cells weui-cells_after-title">
  <block wx:for="{{typeArray}}" wx:for-item="type" wx:key="id">
    <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" bindtap="toProductList" data-index="{{type.id}}">
      <view class="weui-cell__bd">{{type.name}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </view>
  </block>
</view>